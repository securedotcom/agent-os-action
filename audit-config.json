{
  "repositories": [
    "https://github.com/securedotcom/Spring-Backend",
    "https://github.com/securedotcom/spring-fabric",
    "https://github.com/securedotcom/spring-topography-apis",
    "https://github.com/securedotcom/platform-dashboard-apis",
    "https://github.com/securedotcom/siem-agent-provisioning",
    "https://github.com/securedotcom/case_management_pipeline",
    "https://github.com/securedotcom/case-management-backend",
    "https://github.com/securedotcom/Risk-Register",
    "https://github.com/securedotcom/Spring-dashboard",
    "https://github.com/securedotcom/Spring_CIA_algorithm",
    "https://github.com/securedotcom/spring-attack-surface",
    "https://github.com/securedotcom/secure_data_retrieval_agent"
  ],
  "audit_types": [
    "comprehensive",
    "security",
    "quick"
  ],
  "schedule": {
    "comprehensive": "weekly",
    "security": "daily",
    "quick": "on-commit"
  },
  "create_pr": true,
  "git_user": "devatsecure",
  "git_email": "devatsecure@users.noreply.github.com",
  "notification": {
    "enabled": true,
    "channels": ["slack", "email"],
    "slack_webhook": "${SLACK_WEBHOOK_URL}",
    "email_recipients": ["waseem@gaditek.com"]
  },
  "thresholds": {
    "critical_issues": 0,
    "high_priority_issues": 5,
    "test_coverage_minimum": 70
  },
  "auto_fix": {
    "enabled": false,
    "types": ["formatting", "linting", "security-patches"]
  }
}




